@model IEnumerable<Model.EF.Product>







@if (Model.Count() != 0)
{

    <table>
        @for (int i = 0; (i + 1) < Model.Count(); i += 2)
        {

            @:<tr>


                @:<td valign="top" width="50%">




            Html.RenderPartial("ProductDetails", Model.ElementAt(i));


            @:</td>


            @:<td valign="top" width="50%">




            Html.RenderPartial("ProductDetails", Model.ElementAt(i + 1));


            @:</td>


            @:</tr>
        }
    </table>
}

else
{
    @:0
}
